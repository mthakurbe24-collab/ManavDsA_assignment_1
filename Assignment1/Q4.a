#include <iostream> 
using namespace std; 
  
int main() { 
    int arr[1000], n; 
  
    cout << "Enter number of elements: "; 
    cin >> n; 
  
    cout << "Enter array elements: "; 
    for (int i = 0; i < n; i++) { 
        cin >> arr[i]; 
    } 
    for (int i = 0; i < n / 2; i++) { 
        int temp = arr[i]; 
        arr[i] = arr[n - 1 - i]; 
        arr[n - 1 - i] = temp; 
    } 
     cout << "Reversed array: "; 
    for (int i = 0; i < n; i++) { 
        cout << arr[i] << " "; 
    } 
    return 0; 
}